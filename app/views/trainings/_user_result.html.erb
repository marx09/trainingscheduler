<% result = user.results.find_by(serie: serie) %>
<% if result %>
  <button class="btn btn-sm btn-warning btn-edit-result pull-right" data-toggle="modal" data-target="#user_result_modal" data-id="<%= result.id %>" data-user-name="<%= user.first_name %> <%= user.last_name %>" data-excercise="<%= serie.excercise.name %>" data-reps="<%= result.reps %>" data-weight="<%= result.weight %>" data-time="<%= result.time %>"><i class="fa fa-pencil fa-fw"></i></button>
<% else %>
  <button class="btn btn-sm btn-success btn-add-result pull-right" data-toggle="modal" data-target="#user_result_modal" data-user="<%= user.id %>" data-serie="<%= serie.id %>" data-user-name="<%= user.first_name %> <%= user.last_name %>" data-excercise="<%= serie.excercise.name %>"><i class="fa fa-plus fa-fw"></i></button>
<% end %>

<%= user.first_name+" "+user.last_name %><br />
<%= user.email %>
